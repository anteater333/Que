{
  "expo": {
    "name": "Que",
    "slug": "Que",
    "scheme": "queredirect",
    "version": "0.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.que.que-ios-client",
      "config": {
        "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.944223797321-btlbg3kqav2uicn03cpc0qvea1f0fq6j"
        }
      },
      "googleServicesFile": "./firebase/GoogleService-Info.plist",
      "supportsTablet": true
    },
    "android": {
      "package": "com.que.queandroidclient",
      "googleServicesFile": "./firebase/google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    }
  }
}
